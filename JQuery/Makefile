# This Makefile was generated by the Cake3
# https://github.com/grwlf/cake3

GUARD = .cake3/GUARD_$(1)_$(shell echo $($(1)) | md5sum | cut -d ' ' -f 1)

ifeq ($(MAIN),1)
unexport MAIN

# Main section

URCC = $(shell $(shell urweb -print-ccompiler) -print-prog-name=gcc)
URINCL = -I$(shell urweb -print-cinclude) 
.PHONY: all
all: ./Makefile ./lib.urp
./lib.urp: ./../../urscript/lib.urp ./../Uru/lib.urp ./JQuery.ur ./JQuery.urs ./Makefile .cake3/tmp0 autogen/Jquery_1_9_1_js.ur autogen/Jquery_1_9_1_js.urs autogen/Jquery_1_9_1_js_c.h autogen/Jquery_1_9_1_js_c.o
	cat .cake3/tmp0 > ./lib.urp
.cake3/tmp0: ./Makefile
	-rm -rf .cake3/tmp0
	echo 'library ./../../urscript' >> .cake3/tmp0
	echo 'library ./../Uru' >> .cake3/tmp0
	echo 'include autogen/Jquery_1_9_1_js_c.h' >> .cake3/tmp0
	echo 'link autogen/Jquery_1_9_1_js_c.o' >> .cake3/tmp0
	echo 'ffi autogen/Jquery_1_9_1_js_c' >> .cake3/tmp0
	echo 'ffi autogen/Jquery_1_9_1_js_js' >> .cake3/tmp0
	echo 'allow mime text/javascript' >> .cake3/tmp0
	echo 'safeGet Jquery_1_9_1_js/blobpage' >> .cake3/tmp0
	echo 'safeGet Jquery_1_9_1_js/blob' >> .cake3/tmp0
	echo '' >> .cake3/tmp0
	echo 'autogen/Jquery_1_9_1_js' >> .cake3/tmp0
	echo './JQuery' >> .cake3/tmp0
autogen/Jquery_1_9_1_js_c.o: ./Makefile autogen/Jquery_1_9_1_js_c.c $(call GUARD,URCC) $(call GUARD,URINCL) $(call GUARD,UR_CFLAGS)
	$(URCC) -c $(URINCL) $(UR_CFLAGS)  -o autogen/Jquery_1_9_1_js_c.o autogen/Jquery_1_9_1_js_c.c
$(call GUARD,URCC):
	rm -f .cake3/GUARD_URCC_*
	touch $@
$(call GUARD,URINCL):
	rm -f .cake3/GUARD_URINCL_*
	touch $@
$(call GUARD,UR_CFLAGS):
	rm -f .cake3/GUARD_UR_CFLAGS_*
	touch $@

else

# Prebuild/postbuild section

export MAIN=1

ifneq ($(MAKECMDGOALS),clean)

.PHONY: all
all: .fix-multy1
.PHONY: ./lib.urp
./lib.urp: .fix-multy1
.PHONY: .cake3/tmp0
.cake3/tmp0: .fix-multy1
.INTERMEDIATE: .fix-multy1
.fix-multy1: 
	-mkdir .cake3
	$(MAKE) -C ./../../urscript -f Makefile
	$(MAKE) -C ./../Uru -f Makefile
	$(MAKE) -f ./Makefile $(MAKECMDGOALS)
.PHONY: autogen/Jquery_1_9_1_js_c.o
autogen/Jquery_1_9_1_js_c.o: .fix-multy1

endif
.PHONY: clean
clean: 
	-rm ./lib.urp .cake3/tmp0 autogen/Jquery_1_9_1_js_c.o
	-rm -rf .cake3

endif
