# This Makefile was generated by the Cake3
# https://github.com/grwlf/cake3

GUARD = .cake3/GUARD_$(1)_$(shell echo $($(1)) | md5sum | cut -d ' ' -f 1)

ifeq ($(MAIN),1)
unexport MAIN

# Main section

.PHONY: all
all: ./Makefile ./lib.urp
./lib.urp: ./CSS.ur ./CSS.urs ./Makefile ./Uru.ur ./Uru.urs .cake3/tmp0
	cat .cake3/tmp0 > ./lib.urp
.cake3/tmp0: ./Makefile
	-rm -rf .cake3/tmp0
	echo 'allow url http://nivo.dev7studios.com' >> .cake3/tmp0
	echo 'allow url http://plugins.jquery.com/*' >> .cake3/tmp0
	echo '' >> .cake3/tmp0
	echo '$$/list' >> .cake3/tmp0
	echo './CSS' >> .cake3/tmp0
	echo './Uru' >> .cake3/tmp0

else

# Prebuild/postbuild section

export MAIN=1

ifneq ($(MAKECMDGOALS),clean)

.PHONY: all
all: .fix-multy1
.PHONY: ./lib.urp
./lib.urp: .fix-multy1
.PHONY: .cake3/tmp0
.cake3/tmp0: .fix-multy1
.INTERMEDIATE: .fix-multy1
.fix-multy1: 
	-mkdir .cake3
	$(MAKE) -f ./Makefile $(MAKECMDGOALS)

endif
.PHONY: clean
clean: 
	-rm ./lib.urp .cake3/tmp0
	-rm -rf .cake3

endif
